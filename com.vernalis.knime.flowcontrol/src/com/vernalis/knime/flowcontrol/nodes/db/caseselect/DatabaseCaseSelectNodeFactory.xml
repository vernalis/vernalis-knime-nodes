<?xml version="1.0" encoding="UTF-8"?>
<knimeNode icon="./dbselectcase.png" type="Manipulator"
	xmlns="http://knime.org/node/v3.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://knime.org/node/v3.1 http://knime.org/node/v3.1.xsd">
	<name>Database CASE Switch</name>

	<shortDescription>
        Database CASE Switch to alter active branch of database connection ports
    </shortDescription>

	<fullDescription>
		<intro>
			<p>This node provides a database equivalent of the CASE Switch
				node.
			</p>


			<p>
				This node was developed by
				<a href="http://www.vernalis-research.com/">Vernalis Research</a>
				.
				For feedback and more information, please contact
				<a href="mailto:knime@vernalis.com">
					knime@vernalis.com
				</a>
			</p>

		</intro>


		<option name="Select active port">Select the active output port</option>

	</fullDescription>

	<ports>
		<inPort index="0" name="Database connection input port">Input port</inPort>
		<outPort index="0" name="First output port">First output port
		</outPort>
		<outPort index="1" name="Second output port">Second output port
		</outPort>
		<outPort index="2" name="Third output port">Third output port
		</outPort>
	</ports>

</knimeNode>
