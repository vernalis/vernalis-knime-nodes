<knimeNode
	icon="db_list_generic.png"
	type="Manipulator"
	xmlns="http://knime.org/node/v3.1"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://knime.org/node/v3.1 http://knime.org/node/v3.1.xsd">
	<name>DB List Catalogues</name>

	<shortDescription>
		This node lists the catalogues for a database
	</shortDescription>

	<fullDescription>
		<intro>
			<p>This node lists any catalogues for the selected database
			</p>
			<p>
				This node was developed by
				<a href="http://www.vernalis-research.com/">Vernalis Research</a>
				.
				For feedback and more information, please contact
				<a href="mailto:knime@vernalis.com">
					knime@vernalis.com
				</a>
			</p>
		</intro>
	</fullDescription>

	<ports>
		<inPort
			index="0"
			name="Database connection">Database connection
		</inPort>
		<outPort
			index="0"
			name="Catalogues">List of the catalogues
		</outPort>
	</ports>
</knimeNode>
